{
  "extends": [
    "eslint-config-airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style":[
      "warn",
      "as-needed"
    ],
    "no-case-declarations": [
      "warn"
    ],
    "max-len": [
      "warn",
      120,
      {
        "ignoreUrls": true
      }
    ],
    "no-unused-vars": [
      "warn"
    ],
    "array-callback-return": [
      "warn"
    ],
    "react/prefer-stateless-function": [
      "off"
    ]
  }
}